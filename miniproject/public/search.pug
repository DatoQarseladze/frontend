doctype
html
   head
      meta(chartset='utf-8')
      title= title
   body(style={background: '#fff'})
   hr
   #wrapper(style={ display: 'inline-block'})
    a(href="/cars")='Go to add Cars'
    <br>
    a(href="/")='Main page'
    <br>
    form(action="/delete", method='POST')
        h3='Name'
            input(type="text"  value= name name='name')
        h3='Surname'
            input(type="text" value=surname name='surname')
        h3='Personal Number'
            input(type='number'  value=personalNumber name='personalNumber')
        h3='Car Names:'
        each car in cars
            h4= car.manufacter
        button(type='submit')='Delete'